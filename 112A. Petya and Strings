import java.util.*;

public class PetyaStrings {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str1 = sc.next();
        String str2 = sc.next();

        int i = str1.compareToIgnoreCase(str2);
        if (i < -1)
            i = -1;
        else if (i > 1)
            i = 1;

        System.out.println(i);
    }
}
